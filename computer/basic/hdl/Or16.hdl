/**
 * 16-bit Or gate:
 * for i = 0, ..., 15:
 * out[i] = a[i] Or b[i] 
 */
CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    Not16(in=a, out=not1Out);
    Not16(in=b, out=not2Out);
    Nand16(a=not1Out, b= not2Out, out=out);
}