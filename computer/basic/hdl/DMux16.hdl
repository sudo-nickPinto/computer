/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP DMux16 {
    IN in[16], sel;
    OUT a[16], b[16];

    PARTS:
    Extender16(in=sel, out=extendOut);
    Not16(in=extendOut, out=notOut);
    And16(a=in, b=notOut, out=a);
    And16(a=in, b=extendOut, out=b);
}
